{
  "name": "craco-style-resources-loader",
  "version": "0.4.0",
  "description": "A craco plugin to use styles-resources-loader with create-react-app",
  "repository": "github:spacedawwwg/craco-style-resources-loader",
  "main": "index.js",
  "author": "Paul Welsh",
  "license": "MIT",
  "keywords": [
    "craco",
    "create-react-app",
    "react",
    "webpack",
    "css",
    "style-resources-loader"
  ],
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "commitlint": "^9.1.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.1"
  },
  "peerDependencies": {
    "@craco/craco": "^5.6.4",
    "react-scripts": "^3.4.1"
  },
  "dependencies": {
    "style-resources-loader": "^1.3.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "index.js": [
      "eslint --quiet --fix",
      "git add"
    ]
  }
